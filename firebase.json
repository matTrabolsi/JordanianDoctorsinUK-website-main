{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/members/**",
        "destination": "/members/index.html"
      },
      {
        "source": "/electives/**",
        "destination": "/electives/index.html"
      },
      {
        "source": "/conference/**",
        "destination": "/conference/index.html"
      },
      {
        "source": "/about/**",
        "destination": "/about/index.html"
      },
      {
        "source": "/news/**",
        "destination": "/news/index.html"
      },
      {
        "source": "**",
        "destination": "/404.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
}
